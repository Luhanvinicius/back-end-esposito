    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "stripe": "^14.7.0",
    "mercadopago": "^2.1.3",
    "pdf-lib": "^1.17.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
