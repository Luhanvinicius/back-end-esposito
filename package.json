  "description": "Backend para plataforma de análise de documentos imobiliários",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "init-db": "node scripts/init-db.js",
    "create-admin": "node scripts/create-admin.js",
    "migrate:role": "node scripts/run-migration-supabase.js",
